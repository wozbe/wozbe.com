<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="wozbe_admin.manager.configuration_doctrine_orm.class">Wozbe\AdminBundle\Entity\ConfigurationDoctrineOrmManager</parameter>
        <parameter key="wozbe_admin.manager.configuration_dummy.class">Wozbe\AdminBundle\Entity\ConfigurationDummyManager</parameter>
    </parameters>

    <services>
        <service id="wozbe_admin.manager.configuration" alias="wozbe_admin.manager.configuration_doctrine_orm" />
        <service id="wozbe_admin.manager.configuration_doctrine_orm" class="%wozbe_admin.manager.configuration_doctrine_orm.class%" public="false">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument type="service" id="wozbe_admin.repository.configuration" />
        </service>
        <service id="wozbe_admin.manager.configuration_dummy" class="%wozbe_admin.manager.configuration_dummy.class%" public="false" />
    </services>
</container>
