<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="wozbe_admin.manager.configuration.class">Wozbe\AdminBundle\Entity\ConfigurationManager</parameter>
    </parameters>

    <services>
        <service id="wozbe_admin.manager.configuration" class="%wozbe_admin.manager.configuration.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument type="service" id="wozbe_admin.repository.configuration" />
        </service>
    </services>
</container>
