<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="wozbe_admin.repository.configuration.class">Wozbe\AdminBundle\Entity\ConfigurationRepository</parameter>
    </parameters>

    <services>
        <service id="wozbe_admin.repository.configuration" class="%wozbe_admin.repository.configuration.class%"
            factory-service="doctrine.orm.entity_manager" factory-method="getRepository">
          <argument>WozbeAdminBundle:Configuration</argument>
        </service>
    </services>
</container>
